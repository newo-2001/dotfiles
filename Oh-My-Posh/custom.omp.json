{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#9adcff",
          "foreground": "#193549",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#9adcff",
          "foreground": "#193549",
          "style": "powerline",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#9adcff",
          "foreground": "#193549",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "powerline",
          "template": "- {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "background": "#ff92fd",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " \ue5ff {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#f5f263",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_upstream_icon": true,
            "fetch_stash_count": true,
            "branch_icon": "\ue725 "
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}({{ .HEAD }}) {{ if .StashCount }}+{{ .StashCount }} {{ end }}",
          "type": "git"
        },
        {
          "background": "#b3de6e",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf1bc {{ regexReplaceAll \" ?[\\\\(\\\\[\uff08\uff3b][Cc][Vv]\\\\..+?[\\\\)\\\\]\uff09\uff3d]\" .Artist \"\" }} - {{ regexReplaceAll \" ?([\\\\(\\\\[\uff08\uff3b](([Ff]eat\\\\.|[Ww]ith |&|[Ff]rom ).+?|[Rr]adio [Ee]dit)[\\\\)\\\\]\uff09\uff3d]|- [Rr]adio [Ee]dit$)\" .Track \"\" }} ",
          "type": "spotify"
        },
        {
          "background": "#9bc2ff",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uebd8 {{ .Name }} ",
          "type": "az"
        },
        {
          "background": "#6CA35E",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#fc7777",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
