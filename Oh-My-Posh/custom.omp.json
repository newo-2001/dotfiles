{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "patch_pwsh_bleed": true,
  "var": {
    "Spotify": {
      "RemixRegex": "(?:.+? )(?:[-–] (.+?) Remix|[\\(\\[]([^\\)\\]]+?) Remix[\\)\\]])(?:.*)",
      "ArtistRegex": " ?[\\(\\[（［][Cc][Vv][\\.:].+?[\\)\\]）］]",
      "SongRegex": " ?[\\[\\(（［]([^\\)\\]）］]*)[\\)\\]）］]| [-–] .*$"
    }
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background_templates": [
            "{{ if contains .OS \"windows\" }}#9adcff{{ end }}",
            "{{ if contains .OS \"ubuntu\" }}#e95420{{ end }}"
          ],
          "foreground": "#193549",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "parentForeground",
          "background": "parentBackground",
          "style": "powerline",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "parentBackground",
          "foreground": "#e7e35c",
          "style": "powerline",
          "template": "󱐋 ",
          "type": "root"
        },
        {
          "background": "parentBackground",
          "foreground": "#193549",
          "properties": {
            "time_format": "15:04"
          },
          "style": "powerline",
          "template": "- {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "background": "#ff92fd",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " \ue5ff {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#f5f263",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_upstream_icon": true,
            "fetch_stash_count": true,
            "fetch_status": true,
            "branch_icon": "\ue725 "
          },
          "style": "powerline",
          "templates": [
            " {{ .UpstreamIcon }}({{ .HEAD }}) ",
            "{{ if .Ahead }}{{ .Ahead }} {{ end }}",
            "{{ if .Behind }}{{ .Behind }} {{ end }}",
            "{{ if .StashCount }}(\uf187 +{{ .StashCount }}) {{ end }}",
            "{{ if hasPrefix \"WIP\" .Commit.Subject }}\uf0ad {{ end }}"
          ],
          "type": "git"
        },
        {
          "background": "#b3de6e",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "templates": [
            " \uf1bc ",
            "{{ replaceP .Var.Spotify.ArtistRegex (matchP .Var.Spotify.RemixRegex .Track | ternary (replaceP .Var.Spotify.RemixRegex .Track \"$1$2\") .Artist) \"\" }}",
            " - ",
            "{{ replaceP .Var.Spotify.SongRegex .Track \"\" }} "
          ],
          "type": "spotify"
        },
        {
          "background": "#9bc2ff",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uebd8 {{ .Name }} ",
          "type": "az"
        },
        {
          "background": "#6CA35E",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "templates": [
            " \ue718 ",
            "{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}",
            "{{ .Full }} "
          ],
          "type": "node"
        },
        {
          "background": "#fc7777",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
