// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+oem_1",
        "command": "workbench.action.terminal.focus",
        "when": "editorFocus && terminalProcessSupported && !terminalEditorFocus || terminalHasBeenCreated && !terminalEditorFocus && !terminalFocus"
    },
    {
        "key": "ctrl+oem_1",
        "command": "workbench.action.focusFirstEditorGroup",
        "when": "terminalFocus"
    }
]