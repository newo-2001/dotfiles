{
    "workbench.colorCustomizations": {
        "statusBar.background": "#007ACC"
    },
    "workbench.editorAssociations": {
        "*.bin": "hexEditor.hexedit"
    },
    "window.zoomLevel": 1,
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "editor.fontFamily": "JetBrainsMonoNL Nerd Font Mono",
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "terminal.integrated.fontFamily": "JetBrainsMonoNL Nerd Font Mono",
    "terminal.integrated.minimumContrastRatio": 1,
    "powershell.integratedConsole.showOnStartup": false,
    "latex-workshop.view.pdf.ref.viewer": "tabOrBrowser",
    "latex-workshop.view.pdf.viewer": "browser",
    "FSharp.showExplorerOnStartup": false,
    "vim.smartRelativeLine": true,
    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        "<C-p>": false,
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false
    },
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "clippy",
        "--message-format",
        "json",
        "--",
        "-Wclippy::pedantic"
    ],
    "errorLens.excludeBySource": [
        "rust-analyzer(syntax-error)",
        "rustc(Click for full compiler diagnostic)"
    ]
}